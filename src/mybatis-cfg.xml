<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.luckyMoney.LimitMoneyInterface">
    <select id="findById" parameterType="int" resultType="com.example.luckyMoney.LimitConfig">
        select * from `student` where id = #{id}
    </select>

    <insert
            id="insertLimit" parameterType="com.example.luckyMoney.LimitConfig"
            useGeneratedKeys="true">
        insert into student(NAME,AGE) values (${name},#{age})
    </insert>
    <update id="">

    </update>
    <delete id="">
        <where></where>
        <set></set>
        <foreach collection=""></foreach>
    </delete>

</mapper>